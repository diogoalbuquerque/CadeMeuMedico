@model IEnumerable<CadeMeuMedico.Models.Cidades>

@{
    ViewBag.Title = "Index";
}

<h2>Cidades</h2>

<p>
    @Html.ActionLink("Adicionar cidades", "Adicionar")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.ActionLink("Editar", "Editar", new { id = item.IDCidade }) |
                @Html.ActionLink("Details", "Details", new { id = item.IDCidade }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IDCidade })
            </td>
        </tr>
    }

</table>
